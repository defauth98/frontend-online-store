{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "trybe-frontend",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "react/jsx-max-depth": "off"
  },
  "plugins": [
    "react",
    "sonarjs"
  ]
}